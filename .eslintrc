{
  "extends": [
    "airbnb",
    "plugin:jest/recommended"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "max-len": [
      "error",
      {
        "code": 120,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true
      }
    ],
    "semi": [
      "error",
      "never"
    ]
  }
}

